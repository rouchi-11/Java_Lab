import org.w3c.dom.ls.LSOutput;

class Animal{
    void makeSound()
    {
        System.out.println("Animal makes sound");
    }
}
class Dog extends Animal
        {
            @Override
                    void makeSound(){
                System.out.println("DOG BARKS");
                super.makeSound();



            }

        }




public class Final{
    public static void main(String[] args) {
        Dog d=new Dog();
        d.makeSound();

    }
}
